--// PART 16 - SMART AUTOFARM V2
--// Auto Quest + Auto Mob
--// =========================

getgenv().SmartFarm = true

local Player =
game.Players.LocalPlayer



-- Get Level

function GetLevel()

    return Player.Data.Level.Value

end



-- Quest Table

local QuestTable = {

[1] = {

QuestName = "BanditQuest1",
Mob = "Bandit",
Level = 1,
CFrame = CFrame.new(1060,16,1547)

},

[2] = {

QuestName = "MonkeyQuest",
Mob = "Monkey",
Level = 15,
CFrame = CFrame.new(-1602,36,152)

},

[3] = {

QuestName = "PirateQuest",
Mob = "Pirate",
Level = 35,
CFrame = CFrame.new(-1103,14,3895)

},

[4] = {

QuestName = "BruteQuest",
Mob = "Brute",
Level = 45,
CFrame = CFrame.new(-1140,15,4300)

}

}



-- Choose Quest

function SelectQuest()

local lvl = GetLevel()

for i,v in pairs(QuestTable) do

if lvl >= v.Level then

return v

end

end

end



-- Auto Quest

spawn(function()

while SmartFarm do

wait(2)

pcall(function()

local quest =
SelectQuest()

if quest then

Player.Character
.HumanoidRootPart.CFrame =
quest.CFrame

end

end)

end

end)



-- Find Mob

function GetMob(name)

for i,v in pairs(workspace.Enemies:GetChildren()) do

if v.Name == name then

return v

end

end

end



-- Auto Attack Mob

spawn(function()

while SmartFarm do

wait()

pcall(function()

local quest =
SelectQuest()

if quest then

local mob =
GetMob(quest.Mob)

if mob then

Player.Character
.HumanoidRootPart.CFrame =
mob.HumanoidRootPart.CFrame *
CFrame.new(0,15,0)

end

end

end)

end

end)
--// =========================
--// PART 17 - AUTO SKILL V3
--// Smart Combo System
--// =========================

getgenv().AutoSkill = true

local VirtualInput =
game:GetService("VirtualInputManager")



-- Random Delay

function SkillDelay()

    return math.random(1,4)/10

end



-- Press Key

function PressKey(key)

    VirtualInput:SendKeyEvent(
    true,
    key,
    false,
    game
    )

    wait(0.1)

    VirtualInput:SendKeyEvent(
    false,
    key,
    false,
    game
    )

end



-- Skill Combo

local Combo = {

"Z",
"X",
"C",
"V",
"F"

}



-- Auto Combo

spawn(function()

while AutoSkill do

wait(SkillDelay())

pcall(function()

for i,key in pairs(Combo) do

PressKey(key)

wait(SkillDelay())

end

end)

end

end)



-- Random Extra Skill

spawn(function()

while AutoSkill do

wait(math.random(5,15))

pcall(function()

PressKey("Z")

end)

end

end)
