while ESPEnabled do

wait(7)

pcall(function()

for i,v in pairs(workspace:GetChildren()) do

if string.find(v.Name,"Fruit") then

CreateESP(

v.Handle,

"FRUIT",

Color3.fromRGB(0,255,0)

)

end

end

end)

end

end)



-- Chest ESP

spawn(function()

while ESPEnabled do

wait(8)

pcall(function()

for i,v in pairs(workspace:GetDescendants()) do

if v.Name == "Chest1"
or v.Name == "Chest2"
or v.Name == "Chest3" then

