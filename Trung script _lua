local UltraAnti = {}

UltraAnti.Enabled = true


-- Random Delay Generator

function UltraAnti.RandomDelay()

    return math.random(1,5)/10

end



-- Fake Ping System

function UltraAnti.FakePing()

    spawn(function()

        while UltraAnti.Enabled do

            wait(math.random(30,80))

            getgenv().FakePing =
            math.random(30,120)

        end

    end)

end



-- Fake Network Lag

function UltraAnti.FakeLag()

    spawn(function()

        while UltraAnti.Enabled do

            wait(math.random(40,90))

            task.wait(math.random(1,3))

        end

    end)

end



-- Random Movement Pattern

function UltraAnti.RandomMovement()

    spawn(function()

        while UltraAnti.Enabled do

            wait(math.random(50,120))

            pcall(function()

                local hrp =
                game.Players.LocalPlayer
                .Character.HumanoidRootPart

                hrp.CFrame = hrp.CFrame *
                CFrame.new(
                math.random(-2,2),
                0,
                math.random(-2,2)
                )

            end)

        end

    end)

end



-- Random Skill Delay

function UltraAnti.SkillDelay()

    spawn(function()

        while UltraAnti.Enabled do

            wait(math.random(20,50))

            getgenv().SkillDelay =
            UltraAnti.RandomDelay()

        end

    end)

end



-- Random Camera Rotation

function UltraAnti.CameraMove()

    spawn(function()

        while UltraAnti.Enabled do

            wait(math.random(30,90))

            pcall(function()

                workspace.CurrentCamera.CFrame =
                workspace.CurrentCamera.CFrame *
                CFrame.Angles(
                0,
                math.rad(math.random(-5,5)),
                0
                )

            end)

        end

    end)

end



-- Random Script Name

getgenv().ScriptID =
"BF_"..math.random(10000,99999)



