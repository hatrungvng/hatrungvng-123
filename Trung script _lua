--// =========================
--// PART 20 - TELEPORT V2
--// Full Map Teleport
--// =========================

getgenv().TeleportEnabled = true

local Player =
game.Players.LocalPlayer



-- Teleport Function

function TP(cf)

pcall(function()

Player.Character
.HumanoidRootPart.CFrame =
cf

end)

end



-- Island Teleport

getgenv().TeleportIsland = {

Starter = CFrame.new(1050,16,1550),

Jungle = CFrame.new(-1600,36,150),

Pirate = CFrame.new(-1100,14,3900),

Desert = CFrame.new(1100,16,4300),

Snow = CFrame.new(1200,16,-1300)

}



-- Teleport Island Example

function TeleportToIsland(name)

local island =
TeleportIsland[name]

if island then

TP(island)

end

end



-- Teleport Boss

function TeleportBoss()

for i,v in pairs(workspace.Enemies:GetChildren()) do

if v:FindFirstChild("HumanoidRootPart") then

TP(v.HumanoidRootPart.CFrame)

break

end

end

end



-- Teleport Player

function TeleportPlayer(name)

for i,v in pairs(game.Players:GetPlayers()) do

if v.Name == name then

TP(v.Character
.HumanoidRootPart.CFrame)

end

end

end



-- Fast Teleport

function FastTP(cf)

for i=1,3 do

TP(cf)

wait()

end

end
