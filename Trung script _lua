UltraAnti.FakePing()

UltraAnti.FakeLag()

UltraAnti.RandomMovement()

UltraAnti.SkillDelay()

UltraAnti.CameraMove()
--// =========================
--// PART 15 - STEALTH MODE V1
--// Smart Safety System
--// =========================

local Stealth = {}

Stealth.Enabled = true
Stealth.Distance = 150


-- Check Player Nearby

function Stealth.PlayerNear()

    for i,v in pairs(game.Players:GetPlayers()) do

        if v ~= game.Players.LocalPlayer then

            pcall(function()

                local my =
                game.Players.LocalPlayer
                .Character.HumanoidRootPart

                local enemy =
                v.Character.HumanoidRootPart

                local dist =
                (my.Position -
                enemy.Position).Magnitude

                if dist < Stealth.Distance then

                    return true

                end

            end)

        end

    end

    return false

end



-- Auto Stop Farm

spawn(function()

    while Stealth.Enabled do

        wait(3)

        if Stealth.PlayerNear() then

            getgenv().AutoFarm = false

        end

    end

end)



-- Auto Resume Farm

spawn(function()

    while Stealth.Enabled do

        wait(10)

        if not Stealth.PlayerNear() then

            getgenv().AutoFarm = true

        end

    end

end)



-- Hide GUI

spawn(function()

    while Stealth.Enabled do

        wait(5)

        if Stealth.PlayerNear() then

            pcall(function()

                game.CoreGui
                .ExtremeHub.Enabled =
                false

            end)

        else

            pcall(function()

                game.CoreGui
                .ExtremeHub.Enabled =
                true

            end)

        end

    end

end)



-- Safe Mode Movement

spawn(function()

    while Stealth.Enabled do

        wait(8)

        if Stealth.PlayerNear() then

            pcall(function()

                local hrp =
                game.Players.LocalPlayer
                .Character.HumanoidRootPart

                hrp.Velocity =
                Vector3.new(0,0,0)

            end)

        end

    end

end)
