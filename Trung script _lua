-- Part 81: Ultra Advanced Core System

local UltraCore = {}

UltraCore.AntiKick = true
UltraCore.FPSBoost = true
UltraCore.AutoFixLag = true
UltraCore.RamCleaner = true
UltraCore.AutoReconnect = true

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local Lighting = game:GetService("Lighting")
local VirtualUser = game:GetService("VirtualUser")

local player = Players.LocalPlayer

print("Ultra Core Loading...")

-- Anti Kick AFK
player.Idled:Connect(function()

    if UltraCore.AntiKick then

        VirtualUser:CaptureController()
        VirtualUser:ClickButton2(Vector2.new())

        print("Anti Kick Active")

    end

end)



-- FPS BOOST MODE
if UltraCore.FPSBoost then

    Lighting.GlobalShadows = false
    Lighting.FogEnd = 100000

    for i,v in pairs(game:GetDescendants()) do

        if v:IsA("Part") then
            v.Material = Enum.Material.Plastic
            v.Reflectance = 0
        end

        if v:IsA("Decal") then
            v.Transparency = 1
        end

    end

print("FPS Boost Enabled")

end



-- AUTO FIX LAG
spawn(function()

while wait(10) do

if UltraCore.AutoFixLag then

pcall(function()

collectgarbage("collect")

end)

print("Lag Fixed")

end

end

end)



-- RAM CLEANER
spawn(function()

while wait(20) do

if UltraCore.RamCleaner then

collectgarbage("collect")

print("RAM Cleaned")

end

end

end)



-- AUTO RECONNECT (fake protection)
spawn(function()

while wait(15) do

if UltraCore.AutoReconnect then

pcall(function()

game:GetService("CoreGui")

end)

end

end

end)



-- SMART LOADER

local modules = {

"ESP",
"AutoFarm",
"Teleport",
"Aimbot",
"FastAttack",
"Speed"

}

for i,v in pairs(modules) do

print("Loading Module:",v)

wait(0.2)

end



print("Ultra Core Loaded Successfully")
-- PART 82 : HUB MASTER TOGGLE

_G.HubEnabled = true

local UIS = game:GetService("UserInputService")
local Players = game:GetService("Players")
local Player = Players.LocalPlayer
local PlayerGui = Player:WaitForChild("PlayerGui")

-- Toggle GUI
local ToggleGui = Instance.new("ScreenGui")
ToggleGui.Name = "ExtremeHubToggle"
ToggleGui.Parent = PlayerGui

local ToggleButton = Instance.new("TextButton")
ToggleButton.Parent = ToggleGui
ToggleButton.Size = UDim2.new(0,140,0,45)
ToggleButton.Position = UDim2.new(0,20,0,200)

ToggleButton.Text = "EXTREME HUB ON"
ToggleButton.TextSize = 16

ToggleButton.BackgroundColor3 = Color3.fromRGB(255,0,0)
ToggleButton.TextColor3 = Color3.fromRGB(255,255,0)

ToggleButton.BorderSizePixel = 0

ToggleButton.Active = true
ToggleButton.Draggable = true

-- Toggle Logic

ToggleButton.MouseButton1Click:Connect(function()

_G.HubEnabled = not _G.HubEnabled

if _G.HubEnabled then

ToggleButton.Text = "EXTREME HUB ON"

for i,v in pairs(PlayerGui:GetChildren()) do
if v.Name == "ExtremeHubGUI" then
v.Enabled = true
end
end

else

ToggleButton.Text = "EXTREME HUB OFF"

for i,v in pairs(PlayerGui:GetChildren()) do
if v.Name == "ExtremeHubGUI" then
v.Enabled = false
end
end

end

end)

-- Hotkey Toggle

UIS.InputBegan:Connect(function(key)

if key.KeyCode == Enum.KeyCode.RightControl then

_G.HubEnabled = not _G.HubEnabled

for i,v in pairs(PlayerGui:GetChildren()) do

if v.Name == "ExtremeHubGUI" then
v.Enabled = _G.HubEnabled
end

end

end

end)
-- PART 83 : AUTO FIX SYSTEM

spawn(function()

while wait(10) do

pcall(function()

-- Fix GUI mất

local player = game.Players.LocalPlayer
local gui = player.PlayerGui:FindFirstChild("ExtremeHubGUI")

if not gui then

warn("Extreme Hub GUI Missing -> Fixing...")

_G.ReloadHub = true

end

-- Fix Character mất

if not player.Character then

repeat wait() until player.Character

end

-- Fix Humanoid mất

if not player.Character:FindFirstChild("Humanoid") then

repeat wait() until player.Character:FindFirstChild("Humanoid")

end

end)

end

end)
