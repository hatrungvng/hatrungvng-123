--// PART 13 - SMART ANTIDETECT V4
--// Anti Ban PRO
--// =========================

local SmartAnti = {}

SmartAnti.Enabled = true


-- Fake executor name
SmartAnti.Executors = {

    "Synapse X",
    "Fluxus",
    "Krnl",
    "Delta",
    "Codex",
    "Arceus X"

}

getgenv().ExecutorName = SmartAnti.Executors[
math.random(1,#SmartAnti.Executors)
]



-- Fake memory usage
function SmartAnti.FakeMemory()

    spawn(function()

        while SmartAnti.Enabled do

            wait(math.random(20,60))

            collectgarbage()

        end

    end)

end



-- Random WalkSpeed
function SmartAnti.RandomSpeed()

    spawn(function()

        while SmartAnti.Enabled do

            wait(math.random(25,50))

            pcall(function()

                local hum = game.Players.LocalPlayer
                .Character.Humanoid

                hum.WalkSpeed = math.random(14,18)

            end)

        end

    end)

end



-- Random JumpPower
function SmartAnti.RandomJump()

    spawn(function()

        while SmartAnti.Enabled do

            wait(math.random(30,70))

            pcall(function()

                local hum = game.Players.LocalPlayer
                .Character.Humanoid

                hum.JumpPower = math.random(45,55)

            end)

        end

    end)

end



-- Fake player activity
function SmartAnti.PlayerActivity()

    spawn(function()

        while SmartAnti.Enabled do

            wait(math.random(40,90))

            pcall(function()

                game:GetService("VirtualUser")
                :CaptureController()

                game:GetService("VirtualUser")
                :ClickButton1(Vector2.new())

            end)

        end

    end)

end



-- Hide idle kick
function SmartAnti.AntiAFK()

    spawn(function()

        game.Players.LocalPlayer.Idled:Connect(function()

            game:GetService("VirtualUser")
            :CaptureController()

            game:GetService("VirtualUser")
            :ClickButton2(Vector2.new())

        end)

    end)

end



-- Fake stats randomizer
function SmartAnti.FakeStats()

    spawn(function()

        while SmartAnti.Enabled do

            wait(math.random(60,120))

            local fake = math.random(1000,9999)

            getgenv().PlayerCode = fake

        end

    end)

end



-- Random camera zoom
function SmartAnti.CameraZoom()

    spawn(function()

        while SmartAnti.Enabled do

            wait(math.random(20,60))

            pcall(function()

                workspace.CurrentCamera.FieldOfView =
                math.random(60,75)

            end)

        end

    end)

end



-- Start System

SmartAnti.FakeMemory()

SmartAnti.RandomSpeed()

SmartAnti.RandomJump()

SmartAnti.PlayerActivity()

SmartAnti.AntiAFK()

SmartAnti.FakeStats()

SmartAnti.CameraZoom()
