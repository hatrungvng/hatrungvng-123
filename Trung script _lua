--====================================================--
-- PART 4 PRO - AUTO FARM V5
--====================================================--


spawn(function()

while wait() do

if _G.AutoFarm then

pcall(function()


local Player =
game.Players.LocalPlayer

local Char =
Player.Character

local Root =
Char.HumanoidRootPart


-- AUTO EQUIP

if _G.AutoEquip then

local tool =
Player.Backpack:GetChildren()[1]

if tool then

Char.Humanoid:EquipTool(tool)

end

end



-- FIND MOB

local Mob

for i,v in pairs(workspace.Enemies:GetChildren()) do

if v:FindFirstChild("Humanoid") then

if v.Humanoid.Health > 0 then

Mob = v

break

end

end

end



-- TWEEN TO MOB

if Mob then

local Pos =
Mob.HumanoidRootPart.CFrame
* CFrame.new(0,30,0)


FastTween(Pos)


end



end)

end

end

end)






