Players.LocalPlayer



-- PLAYER ESP

spawn(function()

while wait(2) do

if _G.PlayerESP then

for i,v in pairs(Players:GetPlayers()) do

if v ~= Player then

pcall(function()

if v.Character then

if not v.Character:FindFirstChild("ESP") then

local Billboard =
Instance.new("BillboardGui")

Billboard.Name = "ESP"

Billboard.Parent =
v.Character.Head

Billboard.Size =
UDim2.new(0,100,0,40)

Billboard.AlwaysOnTop = true


local Text =
Instance.new("TextLabel")

Text.Parent =
Billboard

Text.Size =
UDim2.new(1,0,1,0)

Text.BackgroundTransparency = 1

Text.TextColor3 =
Color3.fromRGB(255,0,0)

Text.TextScaled = true

Text.Text =
v.Name

end

end

end)

end

end

end

end

end)




