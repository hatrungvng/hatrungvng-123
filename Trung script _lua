-- PART 158 : AUTO COLLECT FRUIT

_G.AutoCollectFruit = false

spawn(function()
    while task.wait(3) do
        if _G.AutoCollectFruit and _G.HubEnabled then
            pcall(function()
                local player = game.Players.LocalPlayer
                local char = player.Character
                if not char then return end
                
                local hrp = char:FindFirstChild("HumanoidRootPart")
                if not hrp then return end
                
                for _, v in pairs(workspace:GetDescendants()) do
                    if v:IsA("Tool") and v:FindFirstChild("Handle") then
                        if v.Name:lower():find("fruit") or v:GetAttribute("Fruit") then
                            local fruitPos = v.Handle.Position
                            if (fruitPos - hrp.Position).Magnitude < 250 then
                                hrp.CFrame = CFrame.new(fruitPos)
                                task.wait(0.5)
                                fireproximityprompt(v:FindFirstChildOfClass("ProximityPrompt"))
                            end
                        end
                    end
                end
            end)
        end
    end
end)

-- PART 159 : AUTO RAID PRO

_G.AutoRaidPro = false

spawn(function()
    while task.wait(5) do
        if _G.AutoRaidPro and _G.HubEnabled then
            pcall(function()
                local player = game.Players.LocalPlayer
                local char = player.Character
                if not char then return end
                
                local hrp = char:FindFirstChild("HumanoidRootPart")
                if not hrp then return end
                
                -- Check if in raid
                local raidZone = workspace:FindFirstChild("RaidBoss")
                if raidZone and raidZone:FindFirstChild("HumanoidRootPart") then
                    hrp.CFrame = raidZone.HumanoidRootPart.CFrame * CFrame.new(0, 10, 5)
                    
                    -- Auto attack boss
                    local tool = char:FindFirstChildOfClass("Tool")
                    if tool then
                        tool:Activate()
                    end
                else
                    -- Start raid if not in raid
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Raids", "StartRaid")
                end
            end)
        end
    end
end)

-- PART 160 : FINAL LOADER ULTIMATE

spawn(function()
    task.wait(3)
    
    -- GUI Notification
    local player = game.Players.LocalPlayer
    local screenGui = Instance.new("ScreenGui")
    local notification = Instance.new("Frame")
    local title = Instance.new("TextLabel")
    local versionText = Instance.new("TextLabel")
    
    screenGui.Parent = player:WaitForChild("PlayerGui")
    notification.Parent = screenGui
    notification.BackgroundColor3 = Color3.fromRGB(20, 20, 30)
    notification.BackgroundTransparency = 0.2
    notification.BorderSizePixel = 0
    notification.Size = UDim2.new(0, 250, 0, 120)
    notification.Position = UDim2.new(1, -260, 1, -140)
    notification.Active = true
    notification.Draggable = true
    
    -- Corner
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 8)
    corner.Parent = notification
    
    -- Gradient
    local gradient = Instance.new("UIGradient")
    gradient.Color = ColorSequence.new({
        ColorSequenceKeypoint.new(0, Color3.fromRGB(40, 40, 60)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(25, 20, 35))
    })
    gradient.Rotation = 90
    gradient.Parent = notification
    
    -- Title
    title.Parent = notification
    title.BackgroundTransparency = 1
    title.Size = UDim2.new(1, 0, 0, 30)
    title.Position = UDim2.new(0, 10, 0, 5)
    title.Text = "🔥 EXTREME BLOX HUB"
    title.TextColor3 = Color3.fromRGB(255, 200, 50)
    title.TextScaled = true
    title.Font = Enum.Font.GothamBold
    title.TextXAlignment = Enum.TextXAlignment.Left
    
    -- Version
    versionText.Parent = notification
    versionText.BackgroundTransparency = 1
    versionText.Size = UDim2.new(1, -20, 0, 70)
    versionText.Position = UDim2.new(0, 10, 0, 35)
    versionText.Text = "✅ LOADED SUCCESSFULLY!\n\n📦 VERSION: ULTIMATE 5.0\n⚡ PARTS: 160/160\n🔰 STATUS: ACTIVE"
    versionText.TextColor3 = Color3.fromRGB(200, 200, 255)
    versionText.TextScaled = true
    versionText.Font = Enum.Font.Gotham
    versionText.TextXAlignment = Enum.TextXAlignment.Left
    versionText.RichText = true
    
    -- Close button
    local closeBtn = Instance.new("TextButton")
    closeBtn.Parent = notification
    closeBtn.BackgroundColor3 = Color3.fromRGB(255, 50, 50)
    closeBtn.Size = UDim2.new(0, 20, 0, 20)
    closeBtn.Position = UDim2.new(1, -25, 0, 5)
    closeBtn.Text = "X"
    closeBtn.TextColor3 = Color3.new(1, 1, 1)
    closeBtn.TextScaled = true
    closeBtn.Font = Enum.Font.GothamBold
    
    local btnCorner = Instance.new("UICorner")
    btnCorner.CornerRadius = UDim.new(0, 4)
    btnCorner.Parent = closeBtn
    
    closeBtn.MouseButton1Click:Connect(function()
        notification.Visible = false
    end)
    
    -- Console final
    print("=":rep(50))
    print("🔥 EXTREME BLOX HUB - ULTIMATE EDITION 🔥")
    print("=":rep(50))
    print("✅ ALL 160 PARTS LOADED SUCCESSFULLY")
    print("📦 VERSION: ULTIMATE 5.0")
    print("⚡ FEATURES: 160/160 ACTIVE")
    print("🔰 STATUS: FULLY OPERATIONAL")
    print("👤 LOADED FOR: " .. player.Name)
    print("=":rep(50))
    print("🎮 READY TO USE | ENJOY FARMING!")
    print("=":rep(50))
    
    -- Auto remove notification after 10 seconds
    task.wait(10)
    if notification then
        notification.Visible = false
    end
end)

print("✅ ALL 160 PARTS LOADED COMPLETE!")
print("🔥 EXTREME BLOX HUB ULTIMATE EDITION 5.0")
print("⚡ 160/160 FEATURES ACTIVATED")

