-- Boss List

local BossList = {

"The Gorilla King",
"Bobby",
"Yeti",
"Vice Admiral",
"Warden",
"Chief Warden",
"Saber Expert"

}



-- Find Boss

function FindBoss()

for i,v in pairs(workspace.Enemies:GetChildren()) do

for b,name in pairs(BossList) do

if v.Name == name then

return v

end

end

end

end



-- Teleport Boss

function TeleportBoss(boss)

pcall(function()

Player.Character
.HumanoidRootPart.CFrame =
boss.HumanoidRootPart.CFrame *
CFrame.new(0,25,0)

end)

end



-- Auto Boss Loop

spawn(function()

while AutoBoss do

wait(1)

pcall(function()

local boss =
FindBoss()

if boss then

TeleportBoss(boss)

end

end)

end

end)



-- Safe Position Above Boss

spawn(function()

while AutoBoss do

wait()

pcall(function()

local boss =
FindBoss()

if boss then

Player.Character
.HumanoidRootPart.CFrame =
boss.HumanoidRootPart.CFrame *
CFrame.new(0,20,0)

end

end)

end

end)



