print("Version : "..HubInfo.Version)
print("Owner : "..HubInfo.Owner)
print("Script ID : "..HubInfo.ScriptID)
print("Discord : "..HubInfo.Discord)
print("====================================")

wait(1)

print("Checking Script...")
wait(1)

print("Loading Functions...")
wait(1)

print("Loading UI...")
wait(1)

print("Starting Hub...")
wait(1)

print(HubInfo.Name.." Loaded Successfully")


--====================================================--
-- SERVICES
