getgenv().AutoFruit = true

local Player =
game.Players.LocalPlayer



-- Find Fruit

function FindFruit()

for i,v in pairs(workspace:GetChildren()) do

if string.find(v.Name,"Fruit") then

return v

end

end

end



-- Teleport Fruit

spawn(function()

while AutoFruit do

wait(2)

pcall(function()

local fruit =
FindFruit()

if fruit then

Player.Character
.HumanoidRootPart.CFrame =
fruit.CFrame

end

end)

end

end)
