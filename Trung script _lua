--====================================================--
-- PART 7 PRO - ANTIBAN GOD SYSTEM
--====================================================--


local Players =
game:GetService("Players")

local Player =
Players.LocalPlayer

local VirtualUser =
game:GetService("VirtualUser")



-- ANTI AFK PRO

spawn(function()

while wait(60) do

if _G.AntiBan then

VirtualUser:CaptureController()

VirtualUser:ClickButton2(Vector2.new())

end

end

end)





-- PLAYER DETECTION


spawn(function()

while wait(5) do

if _G.AntiBan then

for i,v in pairs(Players:GetPlayers()) do

if v ~= Player then

pcall(function()

local d = (
v.Character.HumanoidRootPart.Position -
Player.Character.HumanoidRootPart.Position
).Magnitude


if d < 80 then

_G.AutoFarm = false

wait(10)

end

end)

end

end

end

end

end)






-- FAKE MOVEMENT


spawn(function()

while wait(10) do

if _G.AntiBan then

pcall(function()

Player.Character.Humanoid:Move(

Vector3.new(

math.random(-1,1),

0,

math.random(-1,1)

)

)

end)

end

end

end)






-- RANDOM DELAY


function AntiDelay()

wait(

math.random(1,3)

+ math.random()

)

end






-- MEMORY CLEANER


spawn(function()

while wait(120) do

if _G.AntiBan then

collectgarbage("collect")

end

end

end)






-- AUTO PAUSE


spawn(function()

while wait(6) do

if _G.AntiBan then

for i,v in pairs(Players:GetPlayers()) do

if v ~= Player then

local d =
(
v.Character.HumanoidRootPart.Position -
Player.Character.HumanoidRootPart.Position
).Magnitude

if d < 60 then

_G.AutoFarm = false

end

end

end

end

end

end)






