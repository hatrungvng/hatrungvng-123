--// PART 38 - SAFE FARM
--// =========================

getgenv().SafeFarm = true

local Player =
game.Players.LocalPlayer



spawn(function()

while SafeFarm do

wait(5)

pcall(function()

for i,v in pairs(game.Players:GetPlayers()) do

if v ~= Player then

local dist = (

Player.Character
.HumanoidRootPart.Position -

v.Character
.HumanoidRootPart.Position

).Magnitude

if dist < 200 then

getgenv().AutoFarm =
false

end

end

end

end)

end

end)
