--// =========================
--// PART 12 - ADVANCED ANTIBAN V3
--// Giả lập người chơi thật
--// =========================

local AntiBanV3 = {}

AntiBanV3.Enabled = true

-- Random delay giống người thật
function AntiBanV3.HumanDelay()

    while AntiBanV3.Enabled do

        local delayTime = math.random(2,6)

        wait(delayTime)

    end

end


-- Random nhảy
function AntiBanV3.RandomJump()

    spawn(function()

        while AntiBanV3.Enabled do

            wait(math.random(15,40))

            pcall(function()

                game.Players.LocalPlayer.Character.Humanoid.Jump = true

            end)

        end

    end)

end


-- Random di chuyển nhẹ
function AntiBanV3.MicroMove()

    spawn(function()

        while AntiBanV3.Enabled do

            wait(math.random(10,25))

            pcall(function()

                local char = game.Players.LocalPlayer.Character

                if char then

                    char:TranslateBy(Vector3.new(

                        math.random(-2,2),

                        0,

                        math.random(-2,2)

                    ))

                end

            end)

        end

    end)

end


-- Random quay camera
function AntiBanV3.CameraMove()

    spawn(function()

        while AntiBanV3.Enabled do

            wait(math.random(8,20))

            pcall(function()

                local cam = workspace.CurrentCamera

                cam.CFrame = cam.CFrame *

                CFrame.Angles(

                    0,

                    math.rad(math.random(-5,5)),

                    0

                )

            end)

        end

    end)

end


-- Random pause script
function AntiBanV3.RandomPause()

    spawn(function()

        while AntiBanV3.Enabled do

            wait(math.random(60,120))

            AntiBanV3.Enabled = false

            wait(math.random(5,15))

            AntiBanV3.Enabled = true

        end

    end)

end


-- Random FPS Drop giả lập lag
function AntiBanV3.FakeLag()

    spawn(function()

        while true do

            wait(math.random(50,120))

            task.wait(math.random(0.2,0.6))

        end

    end)

end



-- Start
spawn(AntiBanV3.HumanDelay)
AntiBanV3.RandomJump()
AntiBanV3.MicroMove()
AntiBanV3.CameraMove()
AntiBanV3.RandomPause()
AntiBanV3.FakeLag()
