-- PART 113 : SMART FARM AI V5

_G.SmartFarmV5 = true

spawn(function()

while wait(0.2) do

if _G.SmartFarmV5 and _G.HubEnabled then

pcall(function()

local player = game.Players.LocalPlayer
local char = player.Character

if not char then return end

local hrp =
char:FindFirstChild("HumanoidRootPart")

local bestEnemy = nil
local dist = math.huge

for i,v in pairs(workspace.Enemies:GetChildren()) do

if v:FindFirstChild("Humanoid") then

if v.Humanoid.Health > 0 then

local d =
(hrp.Position -
v.HumanoidRootPart.Position).Magnitude

if d < dist then

dist = d
bestEnemy = v

end

end

end

end

if bestEnemy then

hrp.CFrame =
bestEnemy.HumanoidRootPart.CFrame
* CFrame.new(0,0,7)

end

end)

end

end

end)
