-- PART 102 : AUTO QUEST AI

_G.AutoQuestAI = true

spawn(function()

while wait(5) do

if _G.AutoQuestAI and _G.HubEnabled then

pcall(function()

local level =
game.Players.LocalPlayer.Data.Level.Value

local quest = nil

if level <= 50 then
quest = "BanditQuest1"

elseif level <= 100 then
quest = "MonkeyQuest"

elseif level <= 200 then
quest = "SnowQuest"

else
quest = "SkyQuest"
end

game:GetService("ReplicatedStorage")
.Remotes.CommF_:InvokeServer(
"StartQuest",
quest,
1
)

end)

end

end

end)
