-- Find Chest

function FindChest()

for i,v in pairs(workspace:GetDescendants()) do

if v.Name == "Chest1"
or v.Name == "Chest2"
or v.Name == "Chest3" then

return v

end

end

end



-- Teleport Chest

function TeleportChest(chest)

pcall(function()

Player.Character
.HumanoidRootPart.CFrame =
chest.CFrame +
Vector3.new(0,3,0)

end)

end



