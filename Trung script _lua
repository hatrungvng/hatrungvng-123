local player = game.Players.LocalPlayer
local char = player.Character

if char then

char.HumanoidRootPart.CFrame =
cframe

end

end

-- Example Teleports

Island1 =
CFrame.new(0,50,0)

Island2 =
CFrame.new(3000,50,2000)

Island3 =
CFrame.new(-4000,50,-3000)
-- PART 104 : ESP PRO

